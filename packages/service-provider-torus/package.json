{
  "name": "@tkey/service-provider-torus",
  "version": "16.0.0",
  "description": "TKey Torus Service Provider Module",
  "author": "Torus Labs",
  "homepage": "https://github.com/tkey/tkey#readme",
  "license": "MIT",
  "main": "dist/lib.cjs/index.js",
  "module": "dist/lib.esm/index.js",
  "unpkg": "dist/serviceProviderTorus.umd.min.js",
  "jsdelivr": "dist/serviceProviderTorus.umd.min.js",
  "types": "dist/lib.cjs/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkey/tkey.git"
  },
  "scripts": {
    "test": "cross-env MOCKED=true mocha --config ../../.mocharc.json ",
    "coverage": "nyc npm test",
    "coverage-production": "nyc npm run test-production",
    "test-development": "cross-env MOCKED=false METADATA=http://localhost:5051 mocha --config ../../.mocharc.json ",
    "test-production": "cross-env MOCKED=false METADATA=https://metadata.web3auth.io mocha --config ../../.mocharc.json ",
    "test-debugger": "mocha --config ../../.mocharc.json --inspect-brk",
    "dev": "rimraf dist/ && cross-env NODE_ENV=development torus-scripts build",
    "build": "rimraf dist/ && cross-env NODE_ENV=production torus-scripts build",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepack": "npm run build",
    "pre-commit": "lint-staged"
  },
  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "dependencies": {
    "@tkey/common-types": "^16.0.0",
    "@tkey/service-provider-base": "^16.0.0",
    "@toruslabs/customauth": "^21.0.0",
    "@toruslabs/torus.js": "^16.0.0",
    "bn.js": "^5.2.1",
    "elliptic": "^6.6.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/elliptic": "^6.4.18"
  },
  "bugs": {
    "url": "https://github.com/tkey/tkey/issues"
  },
  "lint-staged": {
    "!(*d).ts": [
      "npm run lint --",
      "prettier --write 'src/**/*.ts'"
    ]
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=9.x"
  },
  "gitHead": "4ff88f7fe92b8c85bd0bfde069cde2e56005faca"
}
